<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
    <include layout="@layout/contact_title">

    </include>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:layout_marginTop="12dp"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="80dp">
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"/>
                <LinearLayout
                    android:id="@+id/ll_customer_service"
                    android:background="@drawable/anv_shape_white"
                    android:padding="12dp"
                    android:layout_width="0dp"
                    android:layout_weight="5"
                    android:layout_height="68dp">
                    <ImageView
                        android:layout_gravity="center"
                        android:src="@mipmap/serv_liten"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    <LinearLayout
                        android:layout_marginLeft="20dp"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="match_parent">
                        <TextView
                            android:textColor="@color/send_btn"
                            android:textSize="14sp"
                            android:layout_gravity="center"
                            android:text="客服电话"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                        <TextView
                            android:layout_marginTop="8dp"
                            android:textSize="12sp"
                            android:layout_gravity="center"
                            android:text="400-068-4686"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>
                    <ImageView
                        android:layout_marginRight="12dp"
                        android:src="@drawable/regiht_"
                        android:layout_marginLeft="20dp"
                        android:layout_gravity="center"
                        android:layout_width="20dp"
                        android:layout_height="20dp"/>
                </LinearLayout>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_marginTop="12dp"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="80dp">
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"/>
                <LinearLayout
                        android:id="@+id/ll_wei_xin"
                    android:background="@drawable/anv_shape_white"
                    android:padding="12dp"
                    android:layout_width="0dp"
                    android:layout_weight="5"
                    android:layout_height="68dp">
                    <ImageView
                        android:layout_gravity="center"
                        android:src="@mipmap/wx_serv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    <LinearLayout
                        android:layout_marginLeft="20dp"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="match_parent">
                        <TextView
                            android:textColor="@color/send_btn"
                            android:textSize="14sp"
                            android:layout_gravity="center"
                            android:text="官方微信"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                        <TextView
                            android:layout_marginTop="8dp"
                            android:textSize="12sp"
                            android:layout_gravity="center"
                            android:text="关注公众号咨询"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>
                    <ImageView
                        android:layout_marginRight="12dp"
                        android:src="@drawable/regiht_"
                        android:layout_marginLeft="20dp"
                        android:layout_gravity="center"
                        android:layout_width="20dp"
                        android:layout_height="20dp"/>

                </LinearLayout>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_marginTop="12dp"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="80dp">
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"/>
                <LinearLayout
                    android:id="@+id/ll_qq"
                    android:background="@drawable/anv_shape_white"
                    android:padding="12dp"
                    android:layout_width="0dp"
                    android:layout_weight="5"
                    android:layout_height="68dp">
                    <ImageView
                        android:layout_gravity="center"
                        android:src="@mipmap/qq_ser"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    <LinearLayout
                        android:layout_marginLeft="20dp"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="match_parent">
                        <TextView
                            android:textColor="@color/send_btn"
                            android:textSize="14sp"
                            android:layout_gravity="center"
                            android:text="企业QQ"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                        <TextView
                            android:layout_marginTop="8dp"
                            android:textSize="12sp"
                            android:layout_gravity="center"
                            android:text="4000684686"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>
                    <ImageView
                        android:layout_marginRight="12dp"
                        android:src="@drawable/regiht_"
                        android:layout_marginLeft="20dp"
                        android:layout_gravity="center"
                        android:layout_width="20dp"
                        android:layout_height="20dp"/>

                </LinearLayout>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_marginTop="12dp"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="80dp">
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"/>
                <LinearLayout
                    android:id="@+id/ll_help"
                    android:background="@drawable/anv_shape_white"
                    android:padding="12dp"
                    android:layout_width="0dp"
                    android:layout_weight="5"
                    android:layout_height="68dp">

                    <ImageView
                        android:layout_gravity="center"
                        android:src="@mipmap/fankui"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/imageView2"/>

                    <LinearLayout
                        android:layout_marginLeft="20dp"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="match_parent">
                        <TextView
                            android:textColor="@color/send_btn"
                            android:textSize="14sp"
                            android:layout_gravity="center"
                            android:text="帮助与反馈"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                        <TextView
                            android:visibility="gone"
                            android:layout_marginTop="8dp"
                            android:textSize="14sp"
                            android:layout_gravity="center"
                            android:text="关注公众号咨询"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>
                    <ImageView
                        android:layout_marginRight="12dp"
                        android:src="@drawable/regiht_"
                        android:layout_marginLeft="20dp"
                        android:layout_gravity="center"
                        android:layout_width="20dp"
                        android:layout_height="20dp"/>

                </LinearLayout>
                <TextView
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"/>
            </LinearLayout>
        </LinearLayout>

    </ScrollView>


</LinearLayout>